{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nx: Debug",
      "program": "${workspaceFolder}/node_modules/@nrwl/cli/bin/nx.js",
      "args": ["serve", "soundsauce-nx", "--verbose"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only"],
      "outFiles": ["${workspaceFolder}/dist/out-tsc/**/*.js"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}"
    }
  ]
}
