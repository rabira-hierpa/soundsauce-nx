{
  "name": "soundsauce-nx",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/soundsauce-nx",
  "projectType": "application",
  "tags": ["frontend"],
  "// targets": "to see all targets run: nx show project soundsauce-nx --web",
  "targets": {
    "build": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "soundsauce-nx:build",
        "postcssConfig": "apps/soundsauce-nx/postcss.config.js",
        "tsConfig": "apps/soundsauce-nx/tsconfig.json"
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "soundsauce-nx:build",
        "postcssConfig": "apps/soundsauce-nx/postcss.config.js",
        "port": 4200
      },
      "configurations": {
        "production": {
          "buildTarget": "soundsauce-nx:build:production"
        }
      }
    }
  }
}
